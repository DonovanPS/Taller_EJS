<!DOCTYPE html>
<html lang="en">

<head><%- include ("templates/header.ejs")%></head>

<body>
    <div>
        <%- include ("templates/navbar.ejs")%>
    </div>
    <br>
    <br>
    <div class="container">
        <div class="card">
            <div class="card-header">
                <h1><%= title %></h1>
            </div>
            <div class="card-body">

                <%- include ("templates/table.ejs", { data: data }) %>

                <br>


                <button type="button" class="btn custom-btn" data-bs-toggle="modal" data-bs-target="#exampleModal"
                    onclick="newProduct()">
                    Agregar nuevo producto
                </button>

                <div id="popUp_modal">
                    <%- include ("templates/popUp.ejs", { modalTitle: 'Agregar nuevo producto', modalContent: 'formProduct.ejs' , button: 'Guardar', script: 'nombreSccript' })%>
                </div>

            </div>
        </div>
    </div>


    <script src="js/toast.js"></script>


</body>

</html>